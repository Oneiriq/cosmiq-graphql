{
  "name": "@oneiriq/cosmiq-graphql-deno-examples",
  "version": "1.0.0",
  "description": "Deno examples demonstrating GraphQL server integration with CosmosDB using cosmiq-graphql",
  "license": "MIT",
  "author": "Shon Thomas",
  "tasks": {
    "example:yoga-server": "deno run --allow-net --allow-env --unsafely-ignore-certificate-errors=localhost,127.0.0.1 yoga-server.ts",
    "example:apollo-server": "deno run --allow-net --allow-env --unsafely-ignore-certificate-errors=localhost,127.0.0.1 apollo-server.ts",
    "example:sdl-spec": "deno run --allow-net --allow-env --unsafely-ignore-certificate-errors=localhost,127.0.0.1 cosmosdb-sdl-spec.ts",
    "example:json-spec": "deno run --allow-net --allow-env --unsafely-ignore-certificate-errors=localhost,127.0.0.1 cosmosdb-json-spec.ts"
  },
  "imports": {
    "@azure/cosmos": "npm:@azure/cosmos@^4.9.0",
    "graphql-yoga": "npm:graphql-yoga@^5.17.1",
    "@apollo/server": "npm:@apollo/server@^5.2.0",
    "@oneiriq/cosmiq-graphql": "jsr:@oneiriq/cosmiq-graphql@latest"
  }
}